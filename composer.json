{
	"name": "lasserafn/php-cpr-direkte",
	"description": "A package to use CPR DIREKTE in PHP",
	"keywords": [
		"initials",
		"image",
		"avatar"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		}
	],
    "require": {
		"php": "^7.0|^7.1"
    },
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"InitialAvatarGenerator\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
